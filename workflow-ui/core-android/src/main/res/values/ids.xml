<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- id used by BackStackView to opt in to view persistence. -->
  <item type="id" name="back_stack_view"/>
  <!--
  BackStackView restricts animation to child with this id, if there is one.
  Otherwise animates its entire body.
   -->
  <item type="id" name="back_stack_view_body"/>
  <!-- Id used by ModalContainerView to opt in to view persistence. -->
  <item type="id" name="modal_container_view"/>
  <!-- View Tag that holds the function to update a view created via ViewRegistry. -->
  <item type="id" name="view_show_rendering_function"/>
  <!-- View Tag used for back button handlers. -->
  <item type="id" name="view_back_handler"/>
  <!-- View Tag used for EditText textChangedListener. -->
  <item type="id" name="view_text_changed_listener"/>
  <!-- Default id used by WorkflowLayout to opt in to view persistence. -->
  <item type="id" name="workflow_layout"/>
</resources>
